import numpy as np

age = np.array([23,23,27,27,39,41,47,49,50,52,54,54,56,57,58,58,60,61])

x = 35          
sd_age = 12.94  

minmax = (x - age.min()) / (age.max() - age.min())

zscore = (x - age.mean()) / sd_age

j = len(str(age.max()))
decimal = x / (10**j)

print("Min-Max Normalized:", minmax)
print("Z-Score Normalized:", zscore)
print("Decimal Scaling Normalized:", decimal)
