import pandas as pd
import matplotlib.pyplot as plt

data = {
    "Age":[25,30,35,40,45,50,55,60,65,70],
    "BloodPressure":[72,75,78,82,85,88,90,92,95,98]
}

df = pd.DataFrame(data)

plt.scatter(df["Age"], df["BloodPressure"])
plt.xlabel("Age")
plt.ylabel("Blood Pressure")
plt.title("Blood Pressure vs Age (Scatter Plot)")
plt.show()

plt.bar(df["Age"], df["BloodPressure"])
plt.xlabel("Age")
plt.ylabel("Blood Pressure")
plt.title("Blood Pressure vs Age (Bar Chart)")
plt.show()
