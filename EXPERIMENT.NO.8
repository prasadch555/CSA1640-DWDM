# Data
x <- c(4,1,5,7,10,2,50,25,90,36)
y <- c(12,5,13,19,31,7,153,72,275,110)

# Open PNG device
png("scatter.png")

# Scatter Plot
plot(x, y,
     xlab="Mobile Phones Sold",
     ylab="Money",
     main="Mobile Phones Sold vs Money",
     pch=19)

# Close device
dev.off()

print("Scatter plot saved as scatter.png")
