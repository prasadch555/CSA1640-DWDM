import pandas as pd
import matplotlib.pyplot as plt

url = "https://raw.githubusercontent.com/selva86/datasets/master/mtcars.csv"
mtcars = pd.read_csv(url)

plt.plot(mtcars.index, mtcars["mpg"], label="mpg")
plt.plot(mtcars.index, mtcars["qsec"], label="qsec")

plt.xlabel("Car Index")
plt.ylabel("Values")
plt.title("mpg and qsec from mtcars dataset")
plt.legend()
plt.show()
