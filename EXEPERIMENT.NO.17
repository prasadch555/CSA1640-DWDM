# Load dataset
data(mtcars)

# Plot mpg and qsec in same graph
plot(mtcars$mpg, type="l", col="blue",
     ylab="Value", xlab="Cars",
     main="Multiple Lines: mpg and qsec")

lines(mtcars$qsec, col="red")

# Add legend
legend("topright",
       legend=c("mpg","qsec"),
       col=c("blue","red"),
       lty=1)
