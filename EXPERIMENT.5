import numpy as np
import pandas as pd
import matplotlib.pyplot as plt
import scipy.stats as stats

age = np.array([23,23,27,27,39,41,47,49,50,52,54,54,56,57,58,58,60,61])
fat = np.array([9.5,26.5,7.8,17.8,31.4,25.9,27.4,27.2,31.2,
                34.6,42.5,28.8,33.4,30.2,34.1,32.9,41.2,35.7])

df = pd.DataFrame({"Age":age, "BodyFat":fat})
print("STATISTICS\n")
print(df.describe())


plt.figure(figsize=(8,4))
plt.subplot(1,2,1)
plt.boxplot(age)
plt.title("Age")

plt.subplot(1,2,2)
plt.boxplot(fat)
plt.title("Body Fat %")
plt.show()

plt.scatter(age, fat)
plt.xlabel("Age")
plt.ylabel("Body Fat %")
plt.title("Age vs Body Fat")
plt.show()

plt.figure(figsize=(8,4))
plt.subplot(1,2,1)
stats.probplot(age, plot=plt)
plt.title("Q-Q Plot (Age)")

plt.subplot(1,2,2)
stats.probplot(fat, plot=plt)
plt.title("Q-Q Plot (Body Fat)")
plt.show()
