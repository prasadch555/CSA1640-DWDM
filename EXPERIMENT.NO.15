import numpy as np

min_F = 50000
max_F = 100000
v = 80000

minmax_v = (v - min_F) / (max_F - min_F)
print("Min-Max normalized value of 80000 =", minmax_v)

data = np.array([200,300,400,600,1000])

minmax = (data - data.min()) / (data.max() - data.min())

zscore = (data - data.mean()) / data.std()

print("\nMin-Max Normalized Data:", minmax)
print("Z-Score Normalized Data:", zscore)
