import numpy as np

speed = np.array([78.3,81.8,82,74.2,83.4,84.5,82.9,77.5,80.9,70.6])

Q1 = np.percentile(speed, 25)
Q3 = np.percentile(speed, 75)

IQR = Q3 - Q1

std = np.std(speed, ddof=1)

print("Q1 =", Q1)
print("Q3 =", Q3)
print("Interquartile Range =", IQR)
print("Standard Deviation =", std)
